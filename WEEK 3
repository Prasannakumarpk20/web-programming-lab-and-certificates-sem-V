width: 250px; 
height: 50px; 
font-size: 16px; 
padding: 20px; 
margin: 10px 0; 
border: 1px solid; 
background-color: transparent; 
} 
button[type="submit"] { 
padding: 15px 30px; 
width:250px; 
cursor: non-resizable; 
text-align: center; 
} 
#submitButton{ 
background-color: rgb(0,0,255); 
font-size: 16px; 
color: white; 
} 
input[type="text"]:focus, input[type="password"]:focus { 
outline: none; 
} 
